#!/Users/rubelwi/projects/github/cloudformation-validator/.tox/cov-init/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'cloudformation-validator','console_scripts','cfn-validator'
__requires__ = 'cloudformation-validator'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cloudformation-validator', 'console_scripts', 'cfn-validator')()
    )
